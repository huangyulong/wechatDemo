.log-list {
  display: flex;
  flex-direction: column;
  height: 50vh;
  width: 96%;
  margin-left: 2%;
  margin-top: 10rpx;
  border: 1px solid #ccc;
  border-radius: 8rpx;
}
.log-item {
  color: #333;
  text-indent: 10rpx;
}
.container {
  width: 96%;
  margin: 0 2%;
  background-color: lightcoral;
  /* margin-left: 2%; */
}
.user-container {
  width: 96%;
  margin: 20rpx 2% 0 2%;
  height: 140rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #ccc;
}
.user-image {
  width: 100rpx;
  height: 100rpx;
  border: 1px solid #ccc;
  border-radius: 20rpx;
  margin: 0 20rpx;
}
.user-image image {
  width: 100%;
  height: 100%;
}
.user-name {
  flex: 1;
  color: lightsalmon;
  font-size: 36rpx;
}

.system-container {
  width: 96%;
  margin-left: 2%;
  margin-top: 40rpx;
}
.system-title {
  line-height: 60rpx;
  font-size: 36rpx;
  text-align: center;
}
.system-item {
  line-height: 50rpx;
  font-size: 28rpx;
  border-bottom: 1px solid #ccc;
}
.map-container {
  width:96%;
  margin-left: 2%;
  margin-top: 40rpx;
  border: 1px solid #ccc;
  border-radius: 10rpx;
}
