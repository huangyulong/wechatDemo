.start-container {
  width: 100%;
  /* height: 100rpx; */
  background-color: 'lightblue';
}
.start-login {
  width: 100%;
  height: 50rpx;
  margin-top: 100rpx;
  font-size: 70rpx;
  color: gray;
  text-align: center;
  font-family: 宋体;
}
.start-button {
  width: 100%;
  margin-top: 100px;
  display: flex;
  justify-content: center;
}
.start-button button {
  width: 90%;
}
.start-user {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  margin-top: 200rpx;
}
.start-usertext{
  font-size: 60rpx;
  color: #333;
  margin-top: 20rpx;
  font-family: 仿宋;
}
.start-userimage{
  background-color: #eee;
  width: 400rpx;
  height: 400rpx;
}